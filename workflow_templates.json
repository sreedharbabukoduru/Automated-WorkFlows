      {
        "type": "webhook",
        "name": "API Trigger"
      },
      {
        "type": "http",
        "name": "API Call"
      },
      {
        "type": "transform",
        "name": "Response Processing"
      },
      {
        "type": "action",
        "name": "Result Action"
